@section additionalStyles{
  @if (Model.Company_CoverPhoto == null || Model.Company_CoverPhoto == "")
  {
    Model.Company_CoverPhoto = "~/CompanyImages/default_company_cover.png";
  }
  <style type="text/css">
    .single-category .rating-info > span.col-xs-6, .single-category .rating-info > div.col-xs-6{
    width: 87%;
    margin-left: 40px;
        word-wrap: break-word;
    }
    .compny-profile .profile-bnr {
      min-height: 500px;
      background: url(@Url.Content(Model.Company_CoverPhoto)) no-repeat;
      background-size: contain;
    }
    .compny-profile .profile-bnr .user-info {
      display: flex;
      width: 350px;
      flex-direction: column;
    }
    .compny-profile .profile-bnr .user-info > *{
    align-self: center;
    }
    #social-and-stars{
      display: flex;
    align-items: center;
    justify-content: space-around;
    width: 100%;

    }
    #social-and-stars > .social-links{
    flex 1 1 auto;
    width:initial;
    margin-bottom: 0;
    }

    #social-and-stars > .row{
    flex 1 1 auto;
    width:initial;
    margin-bottom: 5px;
    font-size:1.2em;
    }
    #social-and-stars > .row li{
    width:100%;
    }
    .profile-company-content .sidebar .sidebar-thumbnail img{
    height: 120px;
    }
    .profile-company-content .sidebar .sidebar-information .single-category li{
    margin-bottom: 10px;
    }

  </style>
}

