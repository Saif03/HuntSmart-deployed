@model HuntSmart.Models.Company
@{
  Layout = "~/Views/Shared/_Layout.cshtml";
  ViewBag.Title = "Company - Hunt Smart";
}

<div id="main-wrapper">
  <div class="compny-profile">
    <!-- SUB Banner -->
    <div class="profile-bnr">
      <div class="container">
        <!-- User Iinfo -->
        <div class="user-info">
          <h1>@Model.Company_Name @*<a data-toggle="tooltip" data-placement="top" title="Verified Member"><img src="~/images/icon-ver.png" alt=""></a>*@ </h1>

          <p>
            @Model.Company_Location
          </p>
          <div id="social-and-stars">
            <!-- Social Icon -->
            <div class="social-links">
              @if (@Model.Company_Facebook != "" && @Model.Company_Facebook != null)
              {
                <a href="@Model.Company_Facebook"><i class="fa fa-facebook"></i></a>
              }

              @if (@Model.Company_Twitter != "" && @Model.Company_Twitter != null)
              {
                <a href="@Model.Company_Twitter"><i class="fa fa-twitter"></i></a>
              }

              @if (@Model.Company_LinkedIn != "" && @Model.Company_LinkedIn != null)
              {
                <a href="@Model.Company_LinkedIn"><i class="fa fa-linkedin"></i></a>
              }
            </div>

            <!-- Stars -->
            <ul class="row">
              <li class="col-sm-6">
                <div class="stars">
                  @{
                    int fullStars = Convert.ToInt32(Model.Company_CRating);
                    double? diff = Model.Company_CRating - fullStars;
                    var i = 0;
                    while (i < 5)
                    {
                      if (i < fullStars)
                      {
                        <i class="fa fa-star"></i>
                      }
                      else if (0 < diff && diff < 1 && i < (fullStars + 1))
                      {
                        <i class="fa fa-star-half-o" style="color:#2DABFF"></i>
                      }
                      else
                      {
                        <i class="fa fa-star-o" style="color:#2DABFF"></i>
                      }
                      i++;
                    }
                  }
                  @if (Model.Company_NoOfRaters != null && Model.Company_NoOfRaters != 0)
                  {

                    <span>(@Model.Company_NoOfRaters)</span>
                  }
                  else
                  {
                    <span>(00)</span>

                  }
                </div>
              </li>
            </ul>
          </div>

          <!-- Followers -->
          @*<div class="followr">
            <ul class="row">
            <li class="col-sm-8">
            <p>Followers <span id="followCount">(31)</span></p>
            </li>
            <li class="col-sm-4">
            <p id="followLink">Follow</p>
            </li>
